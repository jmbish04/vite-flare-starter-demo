{
  "name": "vite-flare-starter-demo",
  "version": "0.3.0",
  "private": true,
  "type": "module",
  "description": "Minimal authenticated Cloudflare Workers starter kit with Vite, Hono, React, and better-auth",
  "author": "Jeremy Dawes (Jezweb)",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "tsc && vite build",
    "preview": "wrangler dev",
    "deploy": "wrangler deploy",
    "cf:login": "wrangler login",
    "db:generate": "drizzle-kit generate",
    "db:generate:named": "drizzle-kit generate --name",
    "db:migrate:local": "wrangler d1 migrations apply vite-flare-starter-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply vite-flare-starter-db --remote",
    "db:migrate:list:local": "wrangler d1 migrations list vite-flare-starter-db --local",
    "db:migrate:list:remote": "wrangler d1 migrations list vite-flare-starter-db --remote",
    "db:studio:local": "drizzle-kit studio",
    "db:seed": "npx tsx scripts/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.4",
    "@hookform/resolvers": "^3.10.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-query": "^5.90.10",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "drizzle-orm": "^0.44.7",
    "hono": "^4.10.6",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "react-router-dom": "^7.9.6",
    "resend": "^6.4.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "ua-parser-js": "^2.0.6",
    "zod": "^3.25.76"
  },
  "packageManager": "pnpm@9.0.0",
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.14.2",
    "@cloudflare/vitest-pool-workers": "^0.10.14",
    "@cloudflare/workers-types": "^4.20251115.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/ua-parser-js": "^0.7.39",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "drizzle-kit": "^0.31.7",
    "postcss": "^8.5.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.49.0"
  }
}
